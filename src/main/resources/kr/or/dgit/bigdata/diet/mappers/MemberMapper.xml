<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.dgit.bigdata.diet.mappers.MemberMapper">
	<insert id="insertMember" parameterType="Member">
		insert into member values(#{no}, #{name}, #{gender}, #{weight}, #{age}, #{phone}, #{address}, #{budget});
	</insert>
	
	<select id="selectAllMembers" resultType="Member">
		select no, name, gender, weight, age, phone, address, budget from member;
	</select>
</mapper>